{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off"
    },
    "globals": {
        "describe": "readonly",
        "test": "readonly",
        "expect": "readonly",
        "jest": "readonly"
    }
}
